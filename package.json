{
  "name": "ionic-start",
  "version": "0.0.0",
  "author": "xluobin@163.com",
  "private": true,
  "scripts": {
    "serve": "ionic-app-scripts serve --dev --enableLint false",
    "build": "ionic-app-scripts build --prod",
    "build:aot": "ionic-app-scripts build --prod --aot --minifyjs --minifycss --optimizejs",
    "lint": "ionic-app-scripts lint --typeCheckOnLint true && stylelint \"src/**/*.scss\" --color",
    "precommit": "npm run lint",
    "commitmsg": "validate-commit-msg",
    "pre-release": "standard-version --prerelease",
    "release": "standard-version"
  },
  "dependencies": {
    "@angular/common": "5.0.3",
    "@angular/compiler": "5.0.3",
    "@angular/compiler-cli": "5.0.3",
    "@angular/core": "5.0.3",
    "@angular/forms": "5.0.3",
    "@angular/http": "5.0.3",
    "@angular/platform-browser": "5.0.3",
    "@angular/platform-browser-dynamic": "5.0.3",
    "@ionic-native/core": "4.4.0",
    "@ionic-native/splash-screen": "4.4.0",
    "@ionic-native/status-bar": "4.4.0",
    "@ionic/storage": "2.1.3",
    "@ngrx/effects": "^5.1.0",
    "@ngrx/store": "^5.1.0",
    "@ngrx/store-devtools": "^5.1.0",
    "ionic-angular": "3.9.2",
    "ionicons": "3.0.0",
    "ngrx-store-logger": "^0.2.0",
    "rxjs": "5.5.2",
    "sw-toolbox": "3.6.0",
    "zone.js": "0.8.18"
  },
  "devDependencies": {
    "@ionic/app-scripts": "3.1.8",
    "@types/node": "^8.0.0",
    "codelyzer": "^4.1.0",
    "cordova-android": "7.0.0",
    "cordova-ios": "^4.5.4",
    "cordova-plugin-device": "^2.0.1",
    "cordova-plugin-ionic-keyboard": "^2.0.5",
    "cordova-plugin-ionic-webview": "^1.1.16",
    "cordova-plugin-splashscreen": "^5.0.2",
    "cordova-plugin-whitelist": "^1.3.3",
    "graceful-fs": "^4.1.11",
    "husky": "^0.14.1",
    "standard-version": "^4.3.0",
    "stylelint": "^8.0.0",
    "stylelint-config-standard": "^18.0.0",
    "stylelint-scss": "^2.0.0",
    "tslint": "^5.9.1",
    "typescript": "2.4.2",
    "validate-commit-msg": "^2.12.1"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-device": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-ionic-webview": {},
      "cordova-plugin-ionic-keyboard": {}
    },
    "platforms": [
      "ios",
      "android"
    ]
  }
}
