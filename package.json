{
  "name": "ionic-start",
  "version": "0.0.0",
  "author": "xluobin@163.com",
  "private": true,
  "scripts": {
    "serve": "ionic-app-scripts serve --dev --enableLint false --wwwDir web-dist",
    "build": "ionic-app-scripts build --prod --wwwDir web-dist",
    "build:aot": "ionic-app-scripts build --prod --aot --minifyjs --minifycss --optimizejs --wwwDir web-dist",
    "lint": "ionic-app-scripts lint --typeCheckOnLint true && stylelint \"src/**/*.scss\" --color",
    "precommit": "npm run lint",
    "commitmsg": "validate-commit-msg",
    "pre-release": "standard-version --prerelease",
    "release": "standard-version"
  },
  "dependencies": {
    "@angular/common": "5.0.3",
    "@angular/compiler": "5.0.3",
    "@angular/compiler-cli": "5.0.3",
    "@angular/core": "5.0.3",
    "@angular/forms": "5.0.3",
    "@angular/http": "5.0.3",
    "@angular/platform-browser": "5.0.3",
    "@angular/platform-browser-dynamic": "5.0.3",
    "@ionic-native/core": "4.4.0",
    "@ionic-native/splash-screen": "4.4.0",
    "@ionic-native/status-bar": "4.4.0",
    "@ionic/storage": "2.1.3",
    "ionic-angular": "3.9.2",
    "ionicons": "3.0.0",
    "rxjs": "5.5.2",
    "sw-toolbox": "3.6.0",
    "zone.js": "0.8.18"
  },
  "devDependencies": {
    "@ionic/app-scripts": "3.1.8",
    "@types/node": "^8.0.0",
    "codelyzer": "^4.1.0",
    "husky": "^0.14.1",
    "standard-version": "^4.3.0",
    "stylelint": "^8.0.0",
    "stylelint-config-standard": "^18.0.0",
    "stylelint-scss": "^2.0.0",
    "typescript": "2.4.2",
    "validate-commit-msg": "^2.12.1",
    "webpack-bundle-analyzer": "^2.8.2"
  }
}
