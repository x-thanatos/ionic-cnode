{
  "name": "ionic-start",
  "version": "0.0.0",
  "author": "xluobin@163.com",
  "private": true,
  "scripts": {
    "serve": "ionic-app-scripts serve --dev --enableLint false",
    "build": "ionic-app-scripts build --prod",
    "build:aot": "ionic-app-scripts build --prod --aot --minifyjs --minifycss --optimizejs",
    "run:android": " ionic cordova run android --livereload --debug",
    "lint": "ionic-app-scripts lint --typeCheckOnLint true && stylelint \"src/**/*.scss\" --color",
    "pre-release": "standard-version --prerelease",
    "release": "standard-version"
  },
  "dependencies": {
    "@angular/common": "^6.0.3",
    "@angular/compiler": "^6.0.3",
    "@angular/compiler-cli": "^6.0.3",
    "@angular/core": "^6.0.3",
    "@angular/forms": "^6.0.3",
    "@angular/http": "^6.0.3",
    "@angular/platform-browser": "^6.0.3",
    "@angular/platform-browser-dynamic": "^6.0.3",
    "@ionic-native/core": "^4.7.0",
    "@ionic-native/splash-screen": "^4.7.0",
    "@ionic-native/status-bar": "^4.7.0",
    "@ionic/storage": "^2.1.3",
    "@ngrx/effects": "^6.0.1",
    "@ngrx/store": "^6.0.1",
    "@ngrx/store-devtools": "^6.0.1",
    "cordova-android": "^7.1.0",
    "cordova-ios": "^4.5.4",
    "cordova-plugin-device": "^2.0.2",
    "cordova-plugin-ionic-keyboard": "^2.1.2",
    "cordova-plugin-ionic-webview": "^1.2.1",
    "cordova-plugin-splashscreen": "^5.0.2",
    "cordova-plugin-whitelist": "^1.3.3",
    "graceful-fs": "^4.1.11",
    "ionic-angular": "^3.9.2",
    "ionicons": "^4.1.2",
    "ngrx-store-logger": "^0.2.1",
    "rxjs": "^5.0.0",
    "rxjs-compat": "^6.2.0",
    "sw-toolbox": "^3.6.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@ionic/app-scripts": "^3.1.9",
    "@types/node": "^10.3.0",
    "codelyzer": "^4.3.0",
    "husky": "^0.14.3",
    "standard-version": "^4.4.0",
    "stylelint": "^9.2.1",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-scss": "^3.1.0",
    "tslint": "^5.10.0",
    "typescript": "^2.9.1",
    "validate-commit-msg": "^2.14.0"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-device": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-ionic-webview": {},
      "cordova-plugin-ionic-keyboard": {}
    },
    "platforms": [
      "ios",
      "android"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint",
      "commit-msg": "commitlint --color --edit"
    }
  }
}
