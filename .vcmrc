{
  "types": [
    "feat",
    "fix",
    "docs",
    "style",
    "refactor",
    "example",
    "perf",
    "test",
    "chore",
    "revert"
  ],
  "warnOnFail": false,
  "maxSubjectLength": 72,
  "subjectPattern": ".+",
